programa
{
	
	funcao inicio()
	{

		inteiro cont, tabuada
		
		escreva("Tabuada do 5\n")

		tabuada = 0
		cont = 0
		
		enquanto (cont < 10)
		{
			cont = cont + 1
			tabuada = cont * 5	
			escreva("5 x ",cont, " = ",tabuada,"\n")
		}
		
		
	}
}
